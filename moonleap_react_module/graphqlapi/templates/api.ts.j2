import { always, flow } from 'lodash/fp';
import { doQuery } from 'src/utils/graphqlClient';

{% loop item_list in res.module.store.item_lists %}
{% magicwith item_list.item_name as yellowTulip %}
export function getYellowTulips() {
  const query = `query queryYellowTulips {
    yellowTulips {
      yellowTulip {
        id
        name
      }
    }
  }`;
  const vars = {};
  return doQuery(query, vars).then((response) => {
    return {
      yellowTulips: flow(
        always(response.yellowTulips),
      )(),
    };
  });
}
{% endmagicwith %}
{% endloop %}
