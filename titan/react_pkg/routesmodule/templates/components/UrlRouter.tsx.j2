{% magic_with module.name as moduleName %}
import React from 'react';
import { Router, Switch } from 'react-router-dom';
import { observer } from 'mobx-react-lite';
import { createBrowserHistory } from 'history';
import { Route } from 'src/routes/components';
import { ModuleNameSwitch } from 'src/moduleName/components';                {% for module in _.react_app.modules %}{% ?? module.routes %}{% endfor %}

type PropsT = {};

export const history = createBrowserHistory();

export const UrlRouter: React.FC<PropsT> = observer((props: PropsT) => {
  return (
    <Router history={history}>
      <ModuleNameSwitch />                                                   {% for module in _.react_app.modules %}{% ?? module.routes %}{% endfor %}
    </Router>
  );
});
{% end_magic_with %}
