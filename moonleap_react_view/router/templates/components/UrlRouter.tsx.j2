{% magicwith view.item_name as yellowTulip %}
{% magicwith view.module.name as gardenFlowers %}
import React from 'react';
import { Router, Switch, Route } from 'react-router-dom';
import { observer } from 'mobx-react-lite';
import { createBrowserHistory } from 'history';
{% for view in res.get_views() %}
import { {{ view.name }}, LoadYellowTulipsEffect } from 'src/gardenFlowers/components';
{% endfor %}

type PropsT = {};

export const history = createBrowserHistory();

export const UrlRouter: React.FC<PropsT> = observer((props: PropsT) => {
  return (
    <Router history={history}>
      <Switch>
{% for view in res.get_views() %}
        <Route path="/yellowTulips">
          <LoadYellowTulipsEffect />
          <{{ view.name }} />
        </Route>
{% endfor %}
      </Switch>
    </Router>
  );
});
{% endmagicwith %}
{% endmagicwith %}
